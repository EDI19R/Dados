frecuencias_cargado = contar_frecuencias(resultados_cargado)

# Histograma para resultados normales
plt.figure(figsize=(12, 6))

# Subplot 1: Resultados normales
plt.subplot(1, 2, 1)
plt.hist(resultados_normal, bins=range(1, 8), color='blue', alpha=0.7, edgecolor='black')
plt.title("Distribución de Resultados Normales")
plt.xlabel("Resultados")
plt.ylabel("Frecuencia")
plt.grid(axis='y', linestyle='--', alpha=0.7)

# Texto con frecuencias para resultados normales
texto_normal = '\n'.join([f"Resultado {k}: {v} veces" for k, v in frecuencias_normal.items()])
plt.text(0.95, 0.95, texto_normal, transform=plt.gca().transAxes,
         verticalalignment='top', horizontalalignment='right',
         bbox=dict(facecolor='white', alpha=0.5))

# Subplot 2: Resultados cargados
