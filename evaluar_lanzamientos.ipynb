# Función para contar las frecuencias de cada resultado
def contar_frecuencias(datos):
    frecuencias = {i: 0 for i in range(1, 7)}
    for resultado in datos:
        if resultado in frecuencias:
            frecuencias[resultado] += 1
    return frecuencias

